{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"allowJs": true,
		"strict": true,
		"noEmit": true,
		"types": ["node", "./virtual.d.ts"],
		"isolatedModules": true,
		"declaration": true
	}
}
